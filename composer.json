{
    "name": "strebl/laravel-league-api",
    "description": "A LeagueWrap bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "leaguewrap", "league of legends", "league wrap", "league api", "league-php", "riot", "riot games", "league of legends API"],
    "authors": [
        {
            "name": "Manuel Strebel",
            "email": "manuel@strebel.xyz",
            "homepage": "https://strebel.xyz"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "illuminate/contracts": "5.2.* || 5.3.*",
        "illuminate/support": "5.2.* || 5.3.*",
        "graham-campbell/manager": "^2.4",
        "league-php/leaguewrap": "^0.7.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Strebl\\LeagueApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strebl\\Tests\\LeagueApi\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
